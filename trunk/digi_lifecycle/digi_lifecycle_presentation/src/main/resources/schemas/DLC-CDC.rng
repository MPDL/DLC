			</zeroOrMore>

			<zeroOrMore>
				<ref name="Pnd_Personen"/>
			</zeroOrMore>

			<zeroOrMore>
				<ref name="Ort"/>
			</zeroOrMore>

			<optional>
				<attribute name="generated">
					<data type="dateTime"/>
				</attribute>
			</optional>

		</element>
	</define>
